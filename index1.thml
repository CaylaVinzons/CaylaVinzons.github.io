@import url(http://fonts.googleapis.com/css?family=Pompiere);
*{
  margin:0;
  padding:0;
}
/*main*/
body{
  background-color:SkyBlue;
  font-family:Pompiere, cursive;
  color:blue;
  font-size:30px;
}
.clearfix:after{
  display:block;
  clear:both; /*if things are floated, go below*/
  content:"";
}
nav, div, ul, li, a, article, h1, p{
  margin:0;
  padding:0;
  border:0;
  background: transparent
  vertical-align:baseline;
  text-decoration: none;
}
nav{
  width:80%;
  height:30%;
  padding:0px 50px;
  margin-left:auto;
  margin-right:auto;
  background-color:Salmon;
  border-radius:20px;
}
nav>div{
  float:left;
  font-size:40px;
  padding:10px 0px;
}
nav>ul{
  list-style-type:none;
}
nav>ul>li{
  float:right;
}
nav>ul>li>a{
  display:block;
  font-size:30px;
  padding:10px 20px;
  text-decoration: none;
  padding-bottom:25px;
  /*border:solid;*/
}
nav>ul>li>a:hover{
  color:white;
  background:black;
}
a:visited{
  color:blue;
}
